{
    "name": "tankist/phalcon-blog-mvc",
    "description": "Phalcon blog MVC example",
    "type": "private",
    "require": {
        "phalcon/devtools": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Gryshko",
            "email": "stalin1945@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Controller\\": "app/controllers/",
            "Model\\": "app/models/",
            "Service\\": "app/services/",
            "Traits\\": "app/traits/",
            "Form\\": "app/forms/",
            "Migration\\": "app/migrations/",
            "Plugin\\": "app/plugins/",
            "Library\\": "library/",
            "Task\\": "app/tasks/",
            "Tag\\": "app/tags/",
            "Widget\\": "app/widgets/",
            "Exception\\": "app/exceptions/"
        }
    },
    "minimum-stability": "dev"
